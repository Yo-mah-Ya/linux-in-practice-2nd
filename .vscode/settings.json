{
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "REST/Develop/AccessControl/CognitoUserPool/client/node_modules": true
  },
  // JS/TS
  "eslint.lintTask.enable": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "npm.packageManager": "pnpm",
  "[typescript][javascript][typescriptreact][javascriptreact][json][jsonc][jsonl][yaml][markdown][html]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enabled": true,
  "cSpell.words": [
    "openapi",
    "Runtimes",
    "pycryptodome",
    "Hostnames",
    "querystring",
    "Mfas"
  ]
}
